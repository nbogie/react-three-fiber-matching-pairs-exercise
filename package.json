{
    "name": "react-three-fiber-matching-pairs",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "description": "Starter code for a simple matching pairs game in 3d",
    "author": "Neill Bogie",
    "scripts": {
        "build": "tsc && vite build",
        "start": "vite --port 3000",
        "format": "prettier --write src index.html",
        "lint": "eslint --max-warnings=0 src",
        "preview": "vite preview",
        "type-check": "tsc --noEmit"
